{
  "name": "home-page-backend",
  "version": "1.0.0",
  "description": "backend for home page app",
  "main": "src/server.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "start": "babel-node src/server.ts --extensions \".ts\"",
    "tsc": "tsc --noEmit",
    "build": "npm run tsc && babel src -d dist --extensions \".ts\"",
    "docker:build": "docker build -t home-page-backend .",
    "docker:run": "docker run --env-file ./.env --rm -p 4000:4000 home-page-backend"
  },
  "author": "Michael Abrahamian",
  "license": "ISC",
  "dependencies": {
    "apollo-datasource-rest": "^3.3.0",
    "apollo-server": "^3.4.0",
    "dotenv": "^10.0.0",
    "graphql": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "jest": "^27.3.1",
    "msw": "^0.35.0",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.4"
  }
}
